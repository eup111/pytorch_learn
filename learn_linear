   "source": [
    "def sgd(params,lr,batch_size):\n",
    "    with torch.no_grad():\n",
    "        for param in params:\n",
    "            param -= lr*param.grad / batch_size\n",
    "            param.grad.zero_()\n",
    "    "
   ]
  },
  {
